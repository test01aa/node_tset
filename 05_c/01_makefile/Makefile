##########################################################
#  Makefile のサンプル
##########################################################

a :=6                # 変数への代入
b =`expr 5 + 2`      # 計算結果の代入
d = 8                # コロンなしでも大丈夫みたい

all:
	@echo Hello World!  #@を付与するとecho back しない 
	@pwd
	@echo $a
	@echo $b
	@echo $d

